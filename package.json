{
  "name": "Demo Game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/material": "^5.15.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "typescript": "^4.9.5",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1"
  },
  "scripts": {
    "api": "node server.js",
    "serve": "npm run build && node server.js",
    "docker:build": "docker build -t demo .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "vite",
    "build": "tsc && vite build",
    "deploy:server": "npm run build && docker build -t demo:v1 . && (docker kill demo && docker rm demo) || docker run -d -p 4001:4001 --name demo demo:v1",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
